\intbl \sb56\sl\sa56 \plain\f2\fs24\cf0\uldb Status Bar\plain\f2\fs24\cf0\v HID_VIEW_STATUS_BAR\cell
\intbl \sb56\sl\sa56 \plain\f2\fs24\cf0 Shows or hides the status bar.\cell\row
\intbl \sb56\sl\sa56 \plain\f2\fs24\cf0\uldb Background\plain\f2\fs24\cf0\v HID_BG_COLOR\cell
\intbl \sb56\sl\sa56 \plain\f2\fs24\cf0 Allows you to change the background color\cell\row
\intbl \sb56\sl\sa56 \plain\f2\fs24\cf0\uldb Full View\plain\f2\fs24\cf0\v HID_VIEW_FULLVIEW\cell
\intbl \sb56\sl\sa56 \plain\f2\fs24\cf0 Make the overlays fill the screen\cell\row
}\page \plain\f0\fs20\cf0 {\cs16\super #{\*\footnote \pard\plain \s15\widctlpar \f4\fs20\lang1031 {\cs16\super #} menu_tools}}\plain\f0\fs20\cf0 {\cs16\super ${\*\footnote \pard\plain \s15\widctlpar \f4\fs20\lang1031 {\cs16\super $} Tools menu commands}}\plain\f0\fs20\cf0 {\cs16\super K{\*\footnote \pard\plain \s15\widctlpar \f4\fs20\lang1031 {\cs16\super K} tools}}\plain\f2\fs24\cf0\b Tool menu commands\par
